<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.XYChart?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
  fx:controller="IncomeExpenseOverviewController">
  <BarChart fx:id="incomeExpenseChart" prefWidth="600" prefHeight="400">
    <xAxis>
      <CategoryAxis fx:id="xAxis" label="Month" />
    </xAxis>
    <yAxis>
      <NumberAxis fx:id="yAxis" label="Amount" />
    </yAxis>
  </BarChart>

  <!-- comment: Rexx program that stores all fx:id objects in .local~PersonPrinterDialog.fxml
  directory -->
  <fx:script source="put_FXID_objects_into.my.app.rex" />
</AnchorPane>
